        app:layout_constraintTop_toBottomOf="@id/secondLL"
        android:weightSum="5">
        <TextView
            android:id="@+id/history_activity_third_TV"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@color/light_sage"
            android:drawableEnd="@mipmap/ic_comment_black_48px"
            android:paddingEnd="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/fourthLL"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.1428"
        app:layout_constraintTop_toBottomOf="@id/thirdLL"
