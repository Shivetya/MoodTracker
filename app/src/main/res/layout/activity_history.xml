<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.HistoryActivity">

    <LinearLayout
        android:id="@+id/firstLL"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.1428"
        app:layout_constraintTop_toTopOf="parent"
        android:weightSum="5">

        <TextView
            android:id="@+id/history_activity_first_TV"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@color/light_sage"
            android:drawableEnd="@mipmap/ic_comment_black_48px"
            android:paddingEnd="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/secondLL"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.1428"
        app:layout_constraintTop_toBottomOf="@id/firstLL"
        android:weightSum="5">
        <TextView
            android:id="@+id/history_activity_second_TV"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@color/light_sage"
            android:drawableEnd="@mipmap/ic_comment_black_48px"
            android:paddingEnd="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/thirdLL"
