        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.1428"
        app:layout_constraintTop_toBottomOf="@id/fourthLL"
        android:weightSum="5">
        <TextView
            android:id="@+id/history_activity_fifth_TV"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@color/light_sage"
            android:drawableEnd="@mipmap/ic_comment_black_48px"
            android:paddingEnd="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/sixthLL"
